{
    "name": "react-rollup-typescript-babel-boilerplate",
    "private": true,
    "version": "0.1.0",
    "scripts": {
        "lint": "babel-node --config-file ./babel.node.config.js --extensions \".ts\" eng/scripts/lint",
        "type-check": "babel-node --config-file ./babel.node.config.js --extensions \".ts\" eng/scripts/typeCheck",
        "type-check:watch": "babel-node --config-file ./babel.node.config.js --extensions \".ts\" eng/scripts/typeCheckWatch",
        "type-check:test": "babel-node --config-file ./babel.node.config.js --extensions \".ts\" eng/scripts/typeCheckTest",
        "format": "babel-node --config-file ./babel.node.config.js --extensions \".ts\" eng/scripts/format",
        "clean": "babel-node --config-file ./babel.node.config.js --extensions \".ts\" eng/scripts/clean",
        "build": "babel-node --config-file ./babel.node.config.js --extensions \".ts\" eng/scripts/build",
        "start": "babel-node --config-file ./babel.node.config.js --extensions \".ts\" eng/scripts/start",
        "start:mock": "babel-node --config-file ./babel.node.config.js --extensions \".ts\" eng/scripts/startMock",
        "serve": "babel-node --config-file ./babel.node.config.js --extensions \".ts\" eng/scripts/serve",
        "test": "yarn run type-check:test && jest",
        "test:watch": "yarn run type-check:test && jest --watch",
        "test:cov": "yarn run type-check:test && jest --coverage",
        "test:debug": "yarn run type-check:test && node --inspect-brk node_modules/.bin/jest --runInBand"
    },
    "dependencies": {
        "axios": "^0.19.2",
        "core-js": "^3.6.5",
        "lodash-es": "^4.17.15",
        "path-to-regexp": "^1.7.0",
        "proper-url-join": "^2.1.1",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-router-dom": "^5.1.2",
        "regenerator-runtime": "^0.13.5",
        "typestyle": "^2.1.0"
    },
    "devDependencies": {
        "@babel/core": "^7.9.6",
        "@babel/node": "^7.8.7",
        "@babel/plugin-proposal-class-properties": "^7.8.3",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/plugin-transform-modules-commonjs": "^7.9.6",
        "@babel/plugin-transform-parameters": "^7.9.5",
        "@babel/preset-env": "^7.9.6",
        "@babel/preset-react": "^7.9.4",
        "@babel/preset-typescript": "^7.9.0",
        "@jest/globals": "^25.5.2",
        "@svgr/rollup": "^5.4.0",
        "@types/connect-history-api-fallback": "^1.3.3",
        "@types/debug": "^4.1.5",
        "@types/express": "^4.17.6",
        "@types/fs-extra": "^8.1.0",
        "@types/lodash-es": "^4.17.3",
        "@types/node": "^13.13.4",
        "@types/nunjucks": "^3.1.2",
        "@types/proper-url-join": "^2.0.0",
        "@types/react": "^16.9.34",
        "@types/react-dom": "^16.9.7",
        "@types/react-router-dom": "^5.1.5",
        "@types/rollup-plugin-postcss": "^2.0.0",
        "@types/rollup-plugin-url": "^2.2.0",
        "@types/shelljs": "^0.8.7",
        "@typescript-eslint/eslint-plugin": "^2.30.0",
        "@typescript-eslint/parser": "^2.30.0",
        "babel-jest": "^25.5.1",
        "babel-plugin-module-resolver": "^4.0.0",
        "chokidar": "^3.4.0",
        "connect-history-api-fallback": "^1.6.0",
        "debug": "^4.1.1",
        "dotenv": "^8.2.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.3",
        "eslint-plugin-react": "^7.19.0",
        "eslint-plugin-react-hooks": "^4.0.0",
        "express": "^4.17.1",
        "fs-extra": "^9.0.0",
        "globby": "^11.0.0",
        "http-proxy-middleware": "^1.0.3",
        "jest": "^25.5.4",
        "lodash": "^4.17.15",
        "nunjucks": "^3.2.1",
        "prettier": "^2.0.5",
        "rollup": "^2.9.1",
        "@rollup/plugin-babel": "^5.0.0",
        "@rollup/plugin-commonjs": "^11.1.0",
        "@rollup/plugin-json": "^4.0.3",
        "@rollup/plugin-node-resolve": "^7.1.3",
        "rollup-plugin-postcss": "^2.0.3",
        "@rollup/plugin-replace": "^2.3.2",
        "rollup-plugin-svg": "^2.0.0",
        "rollup-plugin-terser": "^5.3.0",
        "@rollup/plugin-url": "^5.0.0",
        "shelljs": "^0.8.4",
        "typescript": "^3.8.3"
    }
}
