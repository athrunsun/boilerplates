{
    "name": "react-webpack-ts-es-boilerplate",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "NODE_ENV=development DEBUG=app:*,eng:* ts-node --project tsconfig.webpack.json -r tsconfig-paths/register eng/dev/server",
        "start:mock": "ENABLE_MOCK=true yarn run start",
        "build": "yarn run clean && yarn install && NODE_ENV=production DEBUG=app:*,eng:* ts-node --project tsconfig.webpack.json -r tsconfig-paths/register eng/prod/build",
        "serve": "NODE_ENV=production DEBUG=app:*,eng:* ts-node --project tsconfig.webpack.json -r tsconfig-paths/register eng/prod/serve",
        "serve:dev": "SERVE_PROD_BUNDLE_ON_DEV=true yarn run serve",
        "clean": "rm -rf dist",
        "test": "jest --watch",
        "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand"
    },
    "dependencies": {
        "@babel/runtime": "^7.3.0",
        "axios": "^0.21.2",
        "lodash": "^4.17.21",
        "loglevel": "^1.6.1",
        "react": "^16.7.0",
        "react-dom": "^16.7.0",
        "typestyle": "^2.0.1",
        "urijs": "^1.19.11"
    },
    "devDependencies": {
        "@babel/core": "^7.2.2",
        "@babel/node": "^7.2.2",
        "@babel/plugin-proposal-class-properties": "^7.3.0",
        "@babel/plugin-transform-regenerator": "^7.0.0",
        "@babel/plugin-transform-runtime": "^7.2.0",
        "@babel/polyfill": "^7.2.5",
        "@babel/preset-env": "^7.3.0",
        "@babel/preset-react": "^7.0.0",
        "awesome-typescript-loader": "^5.2.1",
        "babel-core": "^7.0.0-bridge.0",
        "babel-eslint": "^10.0.1",
        "babel-jest": "^23.6.0",
        "babel-plugin-module-resolver": "^3.1.3",
        "connect-history-api-fallback": "^1.5.0",
        "css-loader": "^1.0.0",
        "debug": "^4.1.1",
        "dotenv": "^6.1.0",
        "eslint": "^5.12.1",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-import-resolver-babel-module": "^4.0.0",
        "eslint-plugin-babel": "^5.3.0",
        "eslint-plugin-flowtype": "^3.2.1",
        "eslint-plugin-import": "^2.14.0",
        "eslint-plugin-jsx-a11y": "^6.1.2",
        "eslint-plugin-prettier": "^3.0.1",
        "eslint-plugin-react": "^7.12.4",
        "express": "^4.17.3",
        "file-loader": "^2.0.0",
        "html-webpack-plugin": "^3.2.0",
        "http-proxy-middleware": "^0.19.0",
        "jest": "^23.6.0",
        "mini-css-extract-plugin": "^0.4.3",
        "prettier": "^1.16.0",
        "serve-static": "^1.13.2",
        "style-loader": "^0.23.0",
        "ts-jest": "^23.10.3",
        "ts-node": "^7.0.1",
        "tsconfig-paths": "^3.6.0",
        "tsconfig-paths-webpack-plugin": "^3.2.0",
        "tslint": "^5.11.0",
        "tslint-config-prettier": "^1.15.0",
        "tslint-react": "^3.6.0",
        "typescript": "^3.1.1",
        "url-loader": "^1.1.1",
        "webpack": "^4.29.0",
        "webpack-dev-middleware": "^3.4.0",
        "webpack-hot-middleware": "^2.24.2",
        "webpack-merge": "^4.1.4"
    }
}
