{
    "name": "react-fusebox-ts-boilerplate",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "NODE_ENV=development DEBUG=app:*,eng:* ts-node -r tsconfig-paths/register fuse",
        "start:mock": "ENABLE_MOCK=true yarn run start",
        "build": "yarn install && NODE_ENV=production DEBUG=app:*,eng:* ts-node -r tsconfig-paths/register fuse build",
        "serve": "NODE_ENV=production DEBUG=app:*,eng:* ts-node -r tsconfig-paths/register eng/prod/serve",
        "serve:dev": "SERVE_PROD_BUNDLE_ON_DEV=true yarn run serve"
    },
    "dependencies": {
        "axios": "^0.18.1",
        "lodash": "^4.17.21",
        "loglevel": "^1.6.1",
        "react": "^16.7.0",
        "react-dom": "^16.7.0",
        "ts-polyfill": "^3.3.3",
        "tslib": "^1.9.3",
        "typestyle": "^2.0.1",
        "urijs": "^1.19.11"
    },
    "devDependencies": {
        "@types/connect-history-api-fallback": "^1.3.1",
        "@types/debug": "^0.0.31",
        "@types/dotenv": "^4.0.3",
        "@types/express": "^4.16.0",
        "@types/http-proxy-middleware": "^0.17.6",
        "@types/lodash": "^4.14.116",
        "@types/loglevel": "^1.5.3",
        "@types/node": "^10.12.2",
        "@types/react": "^16.7.20",
        "@types/react-dom": "^16.0.11",
        "@types/serve-static": "^1.13.2",
        "@types/urijs": "^1.15.38",
        "@typescript-eslint/eslint-plugin": "^1.4.2",
        "@typescript-eslint/parser": "^1.4.2",
        "connect-history-api-fallback": "^1.5.0",
        "debug": "^4.1.1",
        "dotenv": "^6.1.0",
        "eslint": "^5.15.1",
        "eslint-config-prettier": "^4.1.0",
        "eslint-plugin-prettier": "^3.0.1",
        "eslint-plugin-react": "^7.12.4",
        "express": "^4.17.3",
        "fuse-box": "^3.7.1",
        "fuse-box-typechecker": "^2.11.0",
        "http-proxy-middleware": "^0.19.0",
        "prettier": "^1.16.4",
        "serve-static": "^1.13.2",
        "ts-node": "^7.0.1",
        "tsconfig-paths": "^3.6.0",
        "typescript": "^3.2.4",
        "uglify-es": "^3.3.9",
        "uglify-js": "^3.4.9"
    }
}
