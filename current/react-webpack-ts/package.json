{
    "name": "react-webpack-ts-boilerplate",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "format": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\" \"__tests__/**/*.ts\" \"eng/**/*.js\"",
        "type-check": "tsc -p tsconfig.app.json --noEmit",
        "type-check:watch": "tsc -p tsconfig.app.json --noEmit --watch",
        "type-check:test": "tsc -p tsconfig.test.json --noEmit",
        "start": "cross-env NODE_ENV=development CONFIG_ENV=development node eng/dev/server",
        "start:mock": "cross-env REACT_APP_ENABLE_API_MOCK=true yarn run start",
        "start:mock-server": "cross-env CONFIG_ENV=development node eng/mock/server",
        "build": "yarn run clean && yarn install && yarn run type-check && cross-env NODE_ENV=production CONFIG_ENV=production node eng/prod/build",
        "serve": "cross-env NODE_ENV=production CONFIG_ENV=production node eng/prod/serve",
        "clean": "rimraf dist",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
        "lint": "eslint --ext .js,.ts,.tsx \".\""
    },
    "dependencies": {
        "@vanilla-extract/css": "^1.7.2",
        "axios": "^0.27.2",
        "react": "^18.1.0",
        "react-dom": "^18.1.0",
        "react-router-dom": "^6.3.0"
    },
    "devDependencies": {
        "@babel/core": "^7.18.2",
        "@babel/eslint-parser": "^7.18.2",
        "@babel/plugin-proposal-class-properties": "^7.17.12",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/preset-env": "^7.18.2",
        "@babel/preset-react": "^7.17.12",
        "@babel/preset-typescript": "^7.17.12",
        "@jest/globals": "^28.1.0",
        "@types/react": "^18.0.10",
        "@types/react-dom": "^18.0.5",
        "@typescript-eslint/eslint-plugin": "^5.27.0",
        "@typescript-eslint/parser": "^5.27.0",
        "@vanilla-extract/babel-plugin": "^1.1.7",
        "@vanilla-extract/webpack-plugin": "^2.1.10",
        "babel-jest": "^28.1.0",
        "babel-loader": "^8.2.5",
        "babel-plugin-lodash": "^3.3.4",
        "babel-plugin-module-resolver": "^4.1.0",
        "connect-history-api-fallback": "^1.6.0",
        "cross-env": "^7.0.3",
        "css-loader": "^6.7.1",
        "eslint": "^8.16.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-import-resolver-babel-module": "^5.3.1",
        "eslint-plugin-babel": "^5.3.1",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.30.0",
        "eslint-plugin-react-hooks": "^4.5.0",
        "express": "^4.18.1",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^5.5.0",
        "http-proxy-middleware": "^2.0.6",
        "jest": "^28.1.0",
        "mini-css-extract-plugin": "^2.6.0",
        "prettier": "^2.6.2",
        "rimraf": "^3.0.2",
        "style-loader": "^3.3.1",
        "terser-webpack-plugin": "^5.3.3",
        "typescript": "^4.7.2",
        "url-loader": "^4.1.1",
        "webpack": "^5.72.1",
        "webpack-bundle-analyzer": "^4.5.0",
        "webpack-dev-middleware": "^5.3.3",
        "webpack-hot-middleware": "^2.25.1",
        "webpack-merge": "^5.8.0"
    }
}
