{
    "name": "react-rollup-typescript-babel-boilerplate",
    "private": true,
    "version": "0.1.0",
    "scripts": {
        "type-check": "babel-node --config-file ./babel.node.config.js --extensions \".ts\" eng/scripts/typeCheck",
        "type-check:watch": "babel-node --config-file ./babel.node.config.js --extensions \".ts\" eng/scripts/typeCheckWatch",
        "type-check:test": "babel-node --config-file ./babel.node.config.js --extensions \".ts\" eng/scripts/typeCheckTest",
        "format": "babel-node --config-file ./babel.node.config.js --extensions \".ts\" eng/scripts/format",
        "build": "babel-node --config-file ./babel.node.config.js --extensions \".ts\" eng/scripts/buildProd",
        "build:dev": "babel-node --config-file ./babel.node.config.js --extensions \".ts\" eng/scripts/buildDev",
        "start": "babel-node --config-file ./babel.node.config.js --extensions \".ts\" eng/scripts/startDev",
        "start:mock": "babel-node --config-file ./babel.node.config.js --extensions \".ts\" eng/scripts/startMock",
        "start:prod": "babel-node --config-file ./babel.node.config.js --extensions \".ts\" eng/scripts/startProd",
        "clean": "babel-node --config-file ./babel.node.config.js --extensions \".ts\" eng/scripts/clean",
        "test": "yarn run type-check:test && jest",
        "test:watch": "yarn run type-check:test && jest --watch",
        "test:cov": "yarn run type-check:test && jest --coverage",
        "test:debug": "yarn run type-check:test && node --inspect-brk node_modules/.bin/jest --runInBand",
        "lint": "babel-node --config-file ./babel.node.config.js --extensions \".ts\" eng/scripts/lint"
    },
    "dependencies": {
        "axios": "^0.18.1",
        "core-js": "^3.2.1",
        "date-fns": "^2.0.1",
        "dynamic-import-polyfill": "^0.1.0",
        "lodash-es": "^4.17.15",
        "path-to-regexp": "^3.0.0",
        "proper-url-join": "^2.0.1",
        "react": "^16.9.0",
        "react-dom": "^16.9.0",
        "react-router-dom": "^5.0.1",
        "regenerator-runtime": "^0.13.3",
        "typestyle": "^2.0.1"
    },
    "devDependencies": {
        "@babel/core": "^7.6.0",
        "@babel/node": "^7.6.2",
        "@babel/plugin-proposal-class-properties": "^7.3.4",
        "@babel/plugin-syntax-dynamic-import": "^7.2.0",
        "@babel/plugin-transform-modules-commonjs": "^7.6.0",
        "@babel/plugin-transform-parameters": "^7.4.4",
        "@babel/preset-env": "^7.3.4",
        "@babel/preset-react": "^7.0.0",
        "@babel/preset-typescript": "^7.3.3",
        "@svgr/rollup": "^4.3.3",
        "@types/connect-history-api-fallback": "^1.3.3",
        "@types/debug": "^4.1.5",
        "@types/dotenv": "^6.1.1",
        "@types/express": "^4.17.1",
        "@types/http-proxy-middleware": "^0.19.3",
        "@types/jest": "^24.0.18",
        "@types/lodash-es": "^4.17.3",
        "@types/nunjucks": "^3.1.2",
        "@types/react": "^16.9.3",
        "@types/react-dom": "^16.9.1",
        "@types/react-router-dom": "^5.1.0",
        "@types/rollup-plugin-json": "^3.0.2",
        "@types/rollup-plugin-postcss": "^2.0.0",
        "@types/rollup-plugin-url": "^2.2.0",
        "@types/shelljs": "^0.8.5",
        "@typescript-eslint/eslint-plugin": "^2.3.1",
        "@typescript-eslint/parser": "^2.3.1",
        "babel-jest": "^24.9.0",
        "babel-plugin-module-resolver": "^3.2.0",
        "connect-history-api-fallback": "^1.6.0",
        "debug": "^4.1.1",
        "dotenv": "^8.1.0",
        "eslint": "^6.2.2",
        "eslint-config-prettier": "^6.3.0",
        "eslint-plugin-prettier": "^3.1.1",
        "eslint-plugin-react": "^7.14.3",
        "eslint-plugin-react-hooks": "^2.0.1",
        "express": "^4.17.1",
        "fs-extra": "^8.1.0",
        "globby": "^10.0.1",
        "http-proxy-middleware": "^0.20.0",
        "jest": "^24.9.0",
        "lodash": "^4.17.15",
        "nunjucks": "^3.2.0",
        "prettier": "^1.18.2",
        "rollup": "^1.20.2",
        "rollup-plugin-babel": "^4.3.3",
        "rollup-plugin-commonjs": "^10.1.0",
        "rollup-plugin-json": "^4.0.0",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-postcss": "^2.0.3",
        "rollup-plugin-replace": "^2.2.0",
        "rollup-plugin-svg": "^2.0.0",
        "rollup-plugin-terser": "^5.1.1",
        "rollup-plugin-url": "^2.2.2",
        "shelljs": "^0.8.3",
        "typescript": "^3.6.3"
    }
}
