{
    "name": "react-parcel-ts-boilerplate",
    "version": "0.1.0",
    "private": true,
    "alias": {
        "$app": "./src"
    },
    "scripts": {
        "start": "NODE_ENV=development DEBUG=app:*,eng:* ts-node -r tsconfig-paths/register eng/dev/server",
        "start:mock": "ENABLE_MOCK=true yarn run start",
        "build": "yarn install && NODE_ENV=production DEBUG=app:*,eng:* ts-node -r tsconfig-paths/register eng/prod/build",
        "serve": "NODE_ENV=production DEBUG=app:*,eng:* ts-node -r tsconfig-paths/register eng/prod/serve",
        "serve:dev": "SERVE_PROD_BUNDLE_ON_DEV=true yarn run serve"
    },
    "dependencies": {
        "axios": "^0.18.0",
        "lodash": "^4.17.11",
        "loglevel": "^1.6.1",
        "react": "^16.8.3",
        "react-dom": "^16.8.3",
        "ts-polyfill": "^3.3.3",
        "tslib": "^1.9.3",
        "typestyle": "^2.0.1",
        "urijs": "^1.19.1"
    },
    "devDependencies": {
        "@types/connect-history-api-fallback": "^1.3.1",
        "@types/debug": "^0.0.31",
        "@types/dotenv": "^4.0.3",
        "@types/express": "^4.16.0",
        "@types/http-proxy-middleware": "^0.17.6",
        "@types/lodash": "^4.14.116",
        "@types/loglevel": "^1.5.3",
        "@types/node": "^10.12.2",
        "@types/parcel-bundler": "^1.10.2",
        "@types/react": "^16.8.4",
        "@types/react-dom": "^16.8.2",
        "@types/serve-static": "^1.13.2",
        "@types/urijs": "^1.15.38",
        "connect-history-api-fallback": "^1.5.0",
        "debug": "^4.1.1",
        "dotenv": "^6.1.0",
        "express": "^4.16.3",
        "http-proxy-middleware": "^0.19.0",
        "parcel-bundler": "^1.11.0",
        "prettier": "^1.16.4",
        "serve-static": "^1.13.2",
        "ts-node": "^7.0.1",
        "tsconfig-paths": "^3.6.0",
        "tslint": "^5.11.0",
        "tslint-config-prettier": "^1.15.0",
        "tslint-react": "^3.6.0",
        "typescript": "^3.2.4"
    }
}
