{
    "name": "nest-ts-babel-boilerplate",
    "private": true,
    "version": "0.1.0",
    "scripts": {
        "clean": "node eng/scripts/clean",
        "clean:prod": "node eng/scripts/cleanProd",
        "build": "node eng/scripts/build",
        "format": "node eng/scripts/format",
        "start": "node eng/scripts/start",
        "start:debug": "node eng/scripts/startDebug",
        "start:nodemon": "nodemon",
        "start:nodemon:debug": "nodemon --config nodemon-debug.json",
        "start:prod": "node build/prod/main",
        "lint": "node eng/scripts/lint",
