{
    "name": "vue-webpack-es-boilerplate",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "NODE_ENV=development DEBUG=app:*,eng:* babel-node eng/dev/server",
        "start:mock": "ENABLE_MOCK=true yarn run start",
        "build": "yarn run clean && yarn install && NODE_ENV=production DEBUG=app:*,eng:* babel-node eng/prod/build",
        "serve": "NODE_ENV=production DEBUG=app:*,eng:* babel-node eng/prod/serve",
        "serve:dev": "SERVE_PROD_BUNDLE_ON_DEV=true yarn run serve",
        "clean": "rm -rf dist",
        "test": "jest --watch",
        "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand"
    },
    "dependencies": {
        "@babel/polyfill": "^7.2.5",
        "@babel/runtime": "^7.3.0",
        "axios": "^0.18.1",
        "lodash": "^4.17.13",
        "loglevel": "^1.6.1",
        "typestyle": "^2.0.1",
        "urijs": "^1.19.1",
        "vue": "^2.6.6"
    },
    "devDependencies": {
        "@babel/core": "^7.2.2",
        "@babel/node": "^7.2.2",
        "@babel/plugin-proposal-class-properties": "^7.3.4",
        "@babel/plugin-syntax-dynamic-import": "^7.2.0",
        "@babel/plugin-transform-regenerator": "^7.0.0",
        "@babel/plugin-transform-runtime": "^7.2.0",
        "@babel/preset-env": "^7.2.3",
        "@vue/eslint-config-prettier": "^4.0.1",
        "@vue/test-utils": "^1.0.0-beta.20",
        "babel-core": "7.0.0-bridge.0",
        "babel-eslint": "^10.0.1",
        "babel-jest": "^23.6.0",
        "babel-loader": "^8.0.5",
        "babel-plugin-module-resolver": "^3.1.2",
        "connect-history-api-fallback": "^1.5.0",
        "css-loader": "^1.0.0",
        "debug": "^4.1.1",
        "dotenv": "^6.1.0",
        "eslint": "^5.8.0",
        "eslint-plugin-vue": "^5.0.0",
        "express": "^4.16.3",
        "file-loader": "^2.0.0",
        "html-webpack-plugin": "^3.2.0",
        "http-proxy-middleware": "^0.19.0",
        "jest": "^23.6.0",
        "less": "^3.0.4",
        "less-loader": "^4.1.0",
        "mini-css-extract-plugin": "^0.4.3",
        "prettier": "^1.16.0",
        "serve-static": "^1.13.2",
        "style-loader": "^0.23.0",
        "url-loader": "^1.1.1",
        "vue-loader": "^15.7.0",
        "vue-style-loader": "^4.1.2",
        "vue-template-compiler": "^2.5.21",
        "webpack": "^4.29.0",
        "webpack-dev-middleware": "^3.4.0",
        "webpack-hot-middleware": "^2.24.2",
        "webpack-merge": "^4.1.4"
    }
}
