{
    "name": "nest-ts-boilerplate",
    "private": true,
    "version": "0.1.0",
    "scripts": {
        "build": "node eng/scripts/build",
        "format": "node eng/scripts/format",
        "start": "node eng/scripts/start",
        "start:debug": "node eng/scripts/startDebug",
        "start:prod": "node dist/main",
        "lint": "node eng/scripts/lint",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.config.js"
    },
    "dependencies": {
        "@nestjs/common": "^6.5.3",
        "@nestjs/core": "^6.5.3",
        "@nestjs/microservices": "^6.5.3",
        "@nestjs/platform-express": "^6.5.3",
        "@nestjs/websockets": "^6.5.3",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^6.5.2"
    },
    "devDependencies": {
        "@babel/cli": "^7.5.5",
        "@babel/core": "^7.5.5",
        "@babel/preset-env": "^7.5.5",
        "@nestjs/testing": "^6.5.3",
        "@types/debug": "^4.1.5",
        "@types/dotenv": "^6.1.1",
        "@types/express": "^4.17.1",
        "@types/jest": "^24.0.18",
        "@types/node": "^12.7.2",
        "@types/supertest": "^2.0.8",
        "@typescript-eslint/eslint-plugin": "^2.0.0",
        "@typescript-eslint/parser": "^2.0.0",
        "babel-plugin-module-resolver": "^3.2.0",
        "debug": "^4.1.1",
        "dotenv": "^8.1.0",
        "eslint": "^6.3.0",
        "eslint-config-prettier": "^6.1.0",
        "eslint-plugin-prettier": "^3.1.0",
        "jest": "^24.9.0",
        "prettier": "^1.18.2",
        "shelljs": "^0.8.3",
        "supertest": "^4.0.2",
        "ts-jest": "^24.0.2",
        "ts-node": "^8.3.0",
        "tsconfig-paths": "^3.8.0",
        "typescript": "3.5.3"
    }
}
