{
    "name": "react-webpack-mpa-ts-boilerplate",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "NODE_ENV=development DEBUG=app:*,eng:* ts-node --project tsconfig.webpack.json -r tsconfig-paths/register eng/dev/server",
        "start:mock": "ENABLE_MOCK=true yarn run start",
        "build": "yarn run clean && yarn install && NODE_ENV=production DEBUG=app:*,eng:* ts-node --project tsconfig.webpack.json -r tsconfig-paths/register eng/prod/build",
        "serve": "NODE_ENV=production DEBUG=app:*,eng:* ts-node --project tsconfig.webpack.json -r tsconfig-paths/register eng/prod/serve",
        "serve:dev": "SERVE_PROD_BUNDLE_ON_DEV=true yarn run serve",
        "clean": "rm -rf dist",
        "test": "jest --coverage",
        "test:watch": "jest --watch",
        "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand"
    },
    "dependencies": {
        "axios": "^0.18.0",
        "lodash": "^4.17.13",
        "loglevel": "^1.6.1",
        "react": "^16.7.0",
        "react-dom": "^16.7.0",
        "ts-polyfill": "^3.3.3",
        "tslib": "^1.9.3",
        "typestyle": "^2.0.1",
        "urijs": "^1.19.1"
    },
    "devDependencies": {
        "@babel/core": "^7.1.2",
        "@babel/preset-react": "^7.0.0",
        "@types/connect-history-api-fallback": "^1.3.1",
        "@types/debug": "^0.0.31",
        "@types/dotenv": "^4.0.3",
        "@types/express": "^4.16.0",
        "@types/html-webpack-plugin": "^3.2.0",
        "@types/http-proxy-middleware": "^0.17.6",
        "@types/jest": "^23.3.3",
        "@types/lodash": "^4.14.116",
        "@types/loglevel": "^1.5.3",
        "@types/mini-css-extract-plugin": "^0.2.0",
        "@types/node": "^10.12.2",
        "@types/react": "^16.7.20",
        "@types/react-dom": "^16.0.11",
        "@types/serve-static": "^1.13.2",
        "@types/urijs": "^1.15.38",
        "@types/webpack": "^4.4.17",
        "@types/webpack-dev-middleware": "^2.0.2",
        "@types/webpack-hot-middleware": "^2.16.4",
        "@types/webpack-merge": "^4.1.3",
        "@typescript-eslint/eslint-plugin": "^1.4.2",
        "@typescript-eslint/parser": "^1.4.2",
        "awesome-typescript-loader": "^5.2.1",
        "babel-eslint": "^10.0.1",
        "babel-jest": "^23.6.0",
        "connect-history-api-fallback": "^1.5.0",
        "css-loader": "^1.0.0",
        "debug": "^4.1.0",
        "dotenv": "^6.1.0",
        "eslint": "^5.15.1",
        "eslint-config-prettier": "^4.1.0",
        "eslint-plugin-prettier": "^3.0.1",
        "eslint-plugin-react": "^7.12.4",
        "express": "^4.16.3",
        "file-loader": "^2.0.0",
        "html-webpack-plugin": "^3.2.0",
        "http-proxy-middleware": "^0.19.0",
        "jest": "^23.6.0",
        "mini-css-extract-plugin": "^0.4.3",
        "prettier": "^1.16.4",
        "serve-static": "^1.13.2",
        "style-loader": "^0.23.0",
        "ts-jest": "^23.10.3",
        "ts-node": "^7.0.1",
        "tsconfig-paths": "^3.6.0",
        "tsconfig-paths-webpack-plugin": "^3.2.0",
        "typescript": "^3.2.4",
        "url-loader": "^1.1.1",
        "webpack": "^4.29.0",
        "webpack-dev-middleware": "^3.4.0",
        "webpack-hot-middleware": "^2.24.2",
        "webpack-merge": "^4.1.4"
    }
}
